<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lee.myapplication">

    <uses-feature android:name="android.hardware.camera2" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="LIVELETTER"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DesignActivity" />
        <activity android:name=".PhoneActivity" />
        <activity android:name=".TabletActivity" />
        <activity android:name=".CameraActivity" />
        <activity android:name=".TakepictureActivity" />
        <activity android:name=".GallerypictureActivity" />
        <activity android:name=".GalleryhologramActivity" />
        <activity android:name=".PicturehologramActivity" />
        <activity android:name=".TakevideoActivity" />
        <activity android:name=".TvideohologramActivity" />
        <activity android:name=".ChooseformatActivity" />
        <activity android:name=".VideoActivity" />
        <activity android:name=".GvideohologramActivity" />
        <activity android:name=".GalleryvideoActivity"></activity>
    </application>

</manifest>